{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
    "plugins": [
      "transform-runtime",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        },
        "transform-react-jsx",
        {
          "pragma": "React.createElement"
        }
      ]
    ]
}